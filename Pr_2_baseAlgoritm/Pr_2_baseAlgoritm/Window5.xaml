<Window x:Class="Pr_2_baseAlgoritm.Window5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pr_2_baseAlgoritm"
        mc:Ignorable="d"
        Title="Window5" Height="450" Width="800">
    <Grid Margin="0,0,10,-6">
        <Label Content="Сортировка двумерного массива #5" HorizontalAlignment="Left" Margin="30,21,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblArray" Content="Сформировать двумерный массив" HorizontalAlignment="Left" Margin="30,78,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnArray" Content="Выполнить" HorizontalAlignment="Left" Margin="35,117,0,0" VerticalAlignment="Top" Width="96" Height="30" Click="btnArray_Click"/>
        <ListBox x:Name="lbResultaterray" Margin="395,169,10,10"/>
        <Button x:Name="btnSort" Content="Отсортировать массив" HorizontalAlignment="Left" Margin="395,117,0,0" VerticalAlignment="Top" Height="31" Width="82" Click="btnSort_Click"/>
        <ListBox x:Name="lbNewArray" Margin="35,169,461,10"/>
        <Button x:Name="btnClear" Content="Очистить" HorizontalAlignment="Left" Margin="158,116,0,0" VerticalAlignment="Top" Height="31" Width="82" Click="btnClear_Click"/>
    </Grid>
</Window>
